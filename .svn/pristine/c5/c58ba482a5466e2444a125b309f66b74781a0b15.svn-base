<script type="text/javascript">
function memberLogin(mId) {
	if(loginForm.mId.value == "") {
		alert("아이디를 입력하세요");
		loginForm.mId.focus();
		return;
	}
	
	if(loginForm.mPw.value == "") {
		alert("비밀번호를 입력하세요");
		loginForm.mPw.focus();
		return;
	}
	
	loginForm.action = "${pageContext.request.contextPath}/member/login";
	loginForm.submit();
}

function memberMypage(mId) {
	location.href='${pageContext.request.contextPath }/member/update?mId='+mId;

}

</script>
