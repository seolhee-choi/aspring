<body>
<div class="wrapper">
	
	<!--=== End Header ===-->
	<div class="breadcrumbs">
    <div class="container">
        <h1 class="pull-left">LOGIN</h1>
        <ul class="pull-right breadcrumb">
            <li class="active">로그인</li>
            <li><a href="${pageContext.request.contextPath }/member/join">회원가입</a></li>
            <li><a href="${pageContext.request.contextPath }/member/find">ID/PW 찾기</a></li>
        </ul>
    </div>
	</div>

<!-- LoginForm start -->
		<div class="login">
			<div class="form2">
				<c:choose>
					<c:when test="${empty(loginMember)}"><%--비로그인 사용자의 경우 --%>
						<h3>LOGIN</h3><br>
						<p style="color: red;">${message }</p>
						<form class="login-form" name="loginForm" method="post">
							
							<input type="text" name="mId" placeholder="ID"  />
							<input type="password" name="mPw" placeholder="PASSWORD" />
							<button type="button" onclick="memberLogin();">Login</button>
							
							<p class="alternate">
								<a href="${pageContext.request.contextPath }/member/join">회원가입</a>&nbsp;/&nbsp;<a href="${pageContext.request.contextPath }/member/find">ID/PW 찾기</a>
							</p>
						</form>
					</c:when>
					
					<c:otherwise><%--로그인 사용자의 경우 --%>
						<p style="font-size: 30px;">${loginMember.mName }님 환영합니다.</p>
						<input type="hidden" name="mId" value="${member.mId }">
						<br>
						<br>
						<button type="button" id="button1" onclick="location.href='${pageContext.request.contextPath }';">홈으로</button>
						&nbsp;&nbsp;
						<!-- <button type="button" id="button1" onclick="location.href='${pageContext.request.contextPath }/member/update?mId=${member.mId }';">마이페이지</button> -->
						<button type="button" id="button1" onclick="memberMypage('${member.mId}'); ">마이페이지</button> 
						&nbsp;&nbsp;
						<button type="button" id="button2" onclick="location.href='${pageContext.request.contextPath }/member/logout';">로그아웃</button>
					</c:otherwise>
				</c:choose>
			</div>
		</div>
<!-- LoginForm end -->

		<!-- footer -->
		
	</div><!--/wrapper-->

</body>
