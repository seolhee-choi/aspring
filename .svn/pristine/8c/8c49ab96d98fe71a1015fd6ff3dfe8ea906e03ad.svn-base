package site.itwill.dto;

public class RoomCheck {
	private int rmNo;
	private String rmName;
	private int rmPpl;
	private int rmMax;
	private String rmImg;
	private String rmContent;
	private int rmPrice;
	private int rmCheck;
	
	public RoomCheck() {
		// TODO Auto-generated constructor stub
	}

	public int getRmNo() {
		return rmNo;
	}

	public void setRmNo(int rmNo) {
		this.rmNo = rmNo;
	}

	public String getRmName() {
		return rmName;
	}

	public void setRmName(String rmName) {
		this.rmName = rmName;
	}

	public int getRmPpl() {
		return rmPpl;
	}

	public void setRmPpl(int rmPpl) {
		this.rmPpl = rmPpl;
	}

	public int getRmMax() {
		return rmMax;
	}

	public void setRmMax(int rmMax) {
		this.rmMax = rmMax;
	}

	public String getRmImg() {
		return rmImg;
	}

	public void setRmImg(String rmImg) {
		this.rmImg = rmImg;
	}

	public String getRmContent() {
		return rmContent;
	}

	public void setRmContent(String rmContent) {
		this.rmContent = rmContent;
	}

	public int getRmPrice() {
		return rmPrice;
	}

	public void setRmPrice(int rmPrice) {
		this.rmPrice = rmPrice;
	}

	public int getRmCheck() {
		return rmCheck;
	}

	public void setRmCheck(int rmCheck) {
		this.rmCheck = rmCheck;
	}
	
	
	
}
