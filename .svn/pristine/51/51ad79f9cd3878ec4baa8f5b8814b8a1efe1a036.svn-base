/* Top Bank */
.header .topbar ul.topbank {
    top: 25px;
    left: -5px;
    display: none;
    padding: 4px 0;
    padding-left: 0;
    list-style: none;
    min-width: 100px;
    position: absolute;
    background: #f0f0f0;
}

.header .topbar li:hover ul.topbank {
    display: block;
}

.header .topbar ul.topbank:after {
    top: -4px;
    width: 0;
    height: 0;
    left: 8px;
    content: " ";
    display: block;
    position: absolute;
    border-bottom: 6px solid #f0f0f0;
    border-left: 6px solid transparent;
    border-right: 6px solid transparent;
    border-left-style: inset; /*FF fixes*/
    border-right-style: inset; /*FF fixes*/
}

.header .topbar ul.topbank li a {
    color: #555;
    display: block;
    font-size: 10px;
    padding: 2px 12px;
    margin-bottom: 1px;
    text-transform: uppercase;
}

.header .topbar ul.topbank li.active a i {
    color: #999;
    float: right;
    margin-top: 2px;
}

.header .topbar ul.topbank li a:hover,
.header .topbar ul.topbank li.active a {
    background: #fafafa;
}

.header .topbar ul.topbank li a:hover {
    text-decoration: none;
}

