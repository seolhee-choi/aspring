@charset "UTF-8";



*{
    font-family: 'NanumSquare', sans-serif !important;
}
#my-write-list {
	width: 1200px;
	margin: 0 auto 50px auto;
}

#my-write-list .my-write-list-title {
	font-size: 20px;
	color: #333;
	margin-top: 80px;
	text-align: left;
	margin-bottom: 38px
}

#my-write-list .my-write-list-title .my-write-list-title-bar {
	width: 3px;
	background: lime;
	height: 20px;
	display: inline-block;
	vertical-align: middle;
	margin-right: 13px;
	margin-top: -3px
}

#my-write-list .now-table-title {
	text-align: center;
	color: #000;
	margin-top: 60px;
	margin-bottom: 10px;
	font-size: 13px;
	font-weight: bold;
	letter-spacing: -1px;
}

#my-write-list .now-table {
	width: 100%;
	height: 220px;
	background: #f9f9f9;
	padding: 45px 50px;
	font-weight: bold;
	border: 1px solid #e5e5e5;
	box-sizing: border-box
}

#my-write-list .now-table ul {
	width: 100%;
	height: 130px;
	background: #fff;
	border: 1px solid #e5e5e5
}

#my-write-list .now-table ul>li {
	color: #333;
	float: left;
	width: 20%;
	height: 85px;
	border-right: 1px solid #e5e5e5;
	box-sizing: border-box;
	text-align: center;
	margin: 20px 0
}

#my-write-list .now-table ul>li:last-child {
	bordeR: 0px;
}

#my-write-list .now-table p {
	line-height: 40px;
}

#my-write-list .now-table .order-number {
	font-size: 30px;
	font-weight: bold;
	color: #000;
	line-height: 30px
}

#my-write-list .now-table .order-number a {
	color: #777
}

#my-write-list .my-write-list-table {
	width: 100%;
	height: 55px;
	background: #f9f9f9;
	border-bottom: 1px solid #e5e5e5;
	border-left: 1px solid #e5e5e5;
	border-right: 1px solid #e5e5e5;
}

#my-write-list .my-write-list-table td {
	font-size: 13px;
	color: #333;
	font-weight: bold;
	font-family: 'Nanum Gothic'
}

#my-write-list .paging {
	padding-top: 70px
}

#my-write-list .my-write-list-list {
	width: 100%;
	height: 55px;
	background: #fff;
	border-left: 1px solid #e5e5e5;
	border-right: 1px solid #e5e5e5;
}

#my-write-list .my-write-list-list td {
	font-size: 13px;
	color: #333;
	font-weight: normal;
	font-family: 'Nanum Gothic';
	border-bottom: 1px solid #e5e5e5;
	word-break: break-all;
	box-sizing: border-box;
	padding: 10px
}

#my-write-list .my-write-list-txt {
	padding-left: 40px;
	position: relative
}

#my-write-list .my-write-list-txt .lock {
	padding-right: 15px
}

#my-write-list .my-write-list-txt a {
	color: #333
}

#my-write-list .my-write-list-txt a .color {
	color: #ff4898
}

#my-write-list .my-write-list-txt a:hover {
	text-decoration: underline
}

#my-write-list .my-write-list-txt img {
	vertical-align: middle;
	padding-left: 10px
}

/* 페이징 -공통 (170919 KSW) */
.paging {
	width: 100%;
	text-align: center;
	font-size: 0;
	padding: 50px 0
}

.paging>a.paging-number {
	font-size: 15px;
	border-top: 1px solid #e5e5e5;
	border-left: 1px solid #e5e5e5;
	border-bottom: 1px solid #e5e5e5;
	width: 29px;
	height: 29px;
	line-height: 29px;
	display: inline-block;
	box-sizing: border-box;
	color: #888;
	cursor: pointer
}

.paging>a.paging-number:first-child {
	border-left: 0
}

.paging img {
	cursor: pointer;
	vertical-align: top
}

.paging .on {
	border-bottom: 2px solid #00ff00!important
}