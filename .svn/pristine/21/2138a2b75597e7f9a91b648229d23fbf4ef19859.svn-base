			if ( window.innerWidth != WINDOWWIDTH ) {
				setTimeout(function(){ WINDOWWIDTH = window.innerWidth; }, 10);
			}
		});
	});

	function numberWithCommas(x) {
	    return x.toString().replace(/\B(?=(\d{3})+(?!\d))/g, ",");
	}

	function calc() {
		var don=$("#don").text();
		var su=$("#su").val();
		$("#sum").text(numberWithCommas(don*su));
	}
	
	calc();
	
	$("#su").change(calc);
	


</script>

		
	
	
